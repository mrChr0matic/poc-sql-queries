    "FROM poc_db.nyc_taxi_raw\n",
    "WHERE dropoff_latitude IS NOT NULL\n",
    "  AND dropoff_longitude IS NOT NULL\n",
    "GROUP BY dropoff_latitude, dropoff_longitude;\n"
